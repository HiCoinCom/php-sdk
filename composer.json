{
    "name": "chainup-waas/sdk",
    "description": "ChainUp Custody PHP SDK - WaaS and MPC Wallet API Client",
    "type": "library",
    "keywords": ["chainup", "custody", "waas", "mpc", "wallet", "blockchain", "cryptocurrency"],
    "homepage": "https://github.com/ChainUp-Custody/php-sdk",
    "license": "MIT",
    "version": "2.0.0",
    "authors": [
        {
            "name": "ChainUp Custody",
            "email": "custody@chainup.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chainup\\Waas\\Custody\\": "src/Custody/",
            "Chainup\\Waas\\Custody\\Api\\": "src/Custody/Api/",
            "Chainup\\Waas\\Mpc\\": "src/Mpc/",
            "Chainup\\Waas\\Mpc\\Api\\": "src/Mpc/Api/",
            "Chainup\\Waas\\Utils\\": "src/Utils/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chainup\\Waas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
